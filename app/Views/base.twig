{# vairables passed:
    $dataToDisplay data from controllers and models ex: contacts.firstname
    outputType - not valued or valued to PDF indicating a print out
#}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>SkyCliff {% block title %}{% endblock %}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" type="text/css" href="/vendor/fortawesome/font-awesome/css/all.min.css">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz"
            crossorigin="anonymous"></script>

    <link rel="stylesheet" type="text/css" href="/css/main.css">


    {% block stylesheets %}
    {% endblock %}

    {% block javascripts %}
    {% endblock %}
    {#  {{ dump(navHeader) }}  #}
</head>
<body>
<div class="container-fluid">
    <div class="row flex-nowrap">
        <div class="col py-0 px-0">
{#            {% if includeHeaderNav is not defined or includeHeaderNav %}#}
{#                {% include "headerNav.twig" %}#}
{#            {% endif %}#}
            {% if outputType is not defined or outputType != 'PDF' %}
                {% include "headerNav.twig" %}
            {% endif %}

        </div>
    </div>
    <div class="row flex-nowrap">
        {% if outputType is not defined or outputType != 'PDF' %}
            {% include "sideBarNav.twig" %}
        {% endif %}
        <div class="container">
            <div class="row">
                {% include ('pageNav.twig') %}
                <div style="max-width: calc(100% - (100% / 12 * 2)); ">

                    {% block body %}

                    {% endblock %}
                </div>
            </div>
        </div>

    </div>
</div>
{#{% include "footer.twig" %}#}

<!-- Footer -->

{% block modal %} {% endblock %}
{% block modal2 %} {% endblock %}
{% block errorModal %} {% endblock %}
{#{% block modalLogoff %} {% endblock %}#}
{% if outputType is not defined or outputType != 'PDF' %}
    {% include "modal_logoff.twig" %}
    {% include "modal_confirm_delete.twig" %}
{% endif %}

<script src="path/to/bootstrap.min.js"></script>
<script src="/js/perform-event-functions.js"></script>
<script src="/js/button-events.js"></script>
<script src="/js/main.js"></script>
<script src="/js/timeout.js"></script>

</body>
</html>